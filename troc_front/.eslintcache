'1' }}\n          />\n        </div>\n        <div className=\"stats3\">\n          <h2>Visits</h2>\n          <Chart\n            width={'500px'}\n            height={'300px'}\n            chartType=\"GeoChart\"\n            data={[\n              ['Country', 'Visits'],\n              ['France', 50],\n              ['Germany', 2],\n              ['Switzerland', 12],\n              ['Luxembourg', 9],\n              ['Belgium', 23]\n            ]}\n            options={{\n              region: '155',\n              colorAxis: { colors: ['#FFFF00', '#e31b23'] },\n              backgroundColor: '#81d4fa',\n              datalessRegionColor: '##FFFF00',\n              defaultColor: '#C0C0C0',\n            }}\n\n          />\n        </div>\n      </div>\n      <div className=\"dashboardActions\">\n        <Link to={`/seller/${idMain}`}><img src={MyInfos} style={{ width: \"100px\", heigth: \"auto\", marginTop: \"30px\", marginBottom: \"30px\" }} /><br /><p style={{ color: \"black\" }}>PERSONAL INFORMATION</p></Link>\n        <Link to={`/seller/favorites/${idMain}`}><img src={Favorites} style={{ width: \"100px\", heigth: \"auto\", marginTop: \"30px\", marginBottom: \"30px\" }} /><br /><p style={{ color: \"black\" }}>FAVORITES</p></Link>\n        <Link to={`/seller/artworks/${idMain}`}><img src={MyArtworksIcon} style={{ width: \"100px\", heigth: \"auto\", marginTop: \"30px\", marginBottom: \"30px\" }} /><br /><p style={{ color: \"black\" }}>YOUR SALES</p></Link>\n        <Link to={`/seller/stats/${idMain}`}><img style={{ width: \"100px\", heigth: \"auto\", marginTop: \"30px\", marginBottom: \"30px\" }} src={Statistics} /><br /><p style={{ color: \"black\" }}>STATS OFFERS</p></Link>\n        <Link to=\"/seller/post\"><img style={{ width: \"80px\", heigth: \"auto\", marginTop: \"30px\", marginBottom: \"30px\" }} src={CreateArtworks} /><br /><p style={{ color: \"black\" }}>CREATE NEW OFFER</p></Link>\n        <MessagingButton />\n      </div>\n    </div>\n  );\n}\n\nexport default Stats;\n","/home/brahim/C-COD-260-PAR-2-1-ecp-sarah.leleux/troc_front/src/components/Cart/Cart.js",["474"],"import React from 'react';\nimport axios from 'axios';\nimport { Button, Item } from 'semantic-ui-react';\nimport { Link } from 'react-router-dom';\n\nclass Cart extends React.Component {\n\n    state = {\n        userID: localStorage.getItem('id'),\n        cart: [],\n        products: [],\n        cartItems: []\n    }\n\n    async componentDidMount() {\n        this.fetchCart();\n        this.fetchArtworks();\n    }\n\n    async fetchArtworks() {\n        const response = await axios.get(`https://pikassoback.herokuapp.com/artworks`);\n        this.setState({ products: response.data })\n        this.displayProducts()\n    }\n\n    async fetchCart() {\n        const response = await axios.get(`https://pikassoback.herokuapp.com/users/${this.state.userID}`)\n        let cart = response.data.user.cart\n        /* console.log('cart', cart) */\n        this.setState({ cart: cart })\n    }\n\n    getProducts() {\n        var artworks = [];\n        if (this.state.cart.length > 0) {\n            this.state.cart.forEach(async id => {\n                await axios.get(`https://pikassoback.herokuapp.com/artworks/${id}?id=${id}`)\n                    .then(result => artworks.push(result.data.artwork));\n            })\n        }\n        this.setState({ products: artworks });\n\n    }\n\n    displayProducts() {\n        var filter = this.state.products.filter(product => this.state.cart.includes(product._id) == true)\n        this.setState({ cartItems: filter })\n    }\n\n    async deleteCartItem(id) {\n        const response = await axios.get(`https://pikassoback.herokuapp.com/users/${this.state.userID}`);\n        let cart = response.data.user.cart;\n        const index = cart.indexOf(id);\n        if (index > -1) {\n            cart.splice(index, 1);\n        }\n        var config = {\n            method: 'put',\n            url: `https://pikassoback.herokuapp.com/users/${this.state.userID}?id=${this.state.userID}`,\n            headers: {\n                'Content-Type': 'application/json'\n            },\n            data: { cart: cart }\n        };\n        await axios(config)\n        window.location.reload()\n    }\n\n    getTotal() {\n        var total = 0;\n        this.state.cartItems.forEach(item => total += item.price)\n        return total;\n    }\n\n    render() {\n        if (this.state.cartItems.length > 0) {\n            return (\n                <div>\n                    <h1>Cart</h1>\n                    <Item.Group>\n                        {this.state.cartItems.map((item, index) => (\n                            <Item key={index} style={{ display: 'flex', flexDirection: 'row', alignItems: 'center', justifyContent: 'center' }}>\n                                <Item.Image size='tiny' src={item.image} style={{ marginLeft: '15%' }} />\n                                <Item.Content>\n                                    <Item.Header as='a'>{item.title}</Item.Header>\n                                    <Item.Meta>{item.title}</Item.Meta>\n                                </Item.Content>\n                                <Item.Content>\n                                    <Item.Header as='a' style={{ fontSize: '20px' }}>{item.price}€</Item.Header>\n                                </Item.Content>\n                                <Item.Content style={{ marginRight: '10%' }}>\n                                    <Item.Header as='a' onClick={() => this.deleteCartItem(item._id)}>Delete item</Item.Header>\n                                </Item.Content>\n                            </Item>\n                        ))}\n                    </Item.Group>\n                    <hr style={{ width: \"100%\" }}></hr>\n                    <br />\n                    <h1 style={{ textAlign: 'right', marginRight: \"15%\" }}>Total: {this.getTotal()}€</h1>\n                    <Link to='/confirmation'><Button style={{ backgroundColor: \"#dd96c5\" }}>Continue to payment</Button></Link>\n                </div >\n            )\n        } else {\n            return (\n                <div>\n                    <h1>Cart</h1>\n                </div>\n            )\n        }\n    }\n}\n\nexport default Cart;","/home/brahim/C-COD-260-PAR-2-1-ecp-sarah.leleux/troc_front/src/components/Cart/Confirmation.js",[],"/home/brahim/C-COD-260-PAR-2-1-ecp-sarah.leleux/troc_front/src/components/artists/artistList.js",["475","476"],"import React from 'react'\nimport { Button, Header, Card, Image, } from 'semantic-ui-react'\nimport axios from 'axios';\nimport { Link } from 'react-router-dom';\nimport './artistList.css'\n\n\n\nclass artistList extends React.Component {\n\n    state = {\n        userList: [],\n        artistList: []\n    }\n\n    async getAllArtists() {\n        await axios.get('https://pikassoback.herokuapp.com/users')\n            .then(res =>\n                this.setState({\n                    userList: res.data.users\n                })\n            )\n        /* console.log(this.state.userList) */\n    }\n\n    componentDidMount() {\n        this.getAllArtists();\n    }\n\n    render() {\n\n        return (\n            <div className=\"bod\">\n\n                <div >\n\n                    <h1>Artists List</h1>\n                    <div class=\"container\">\n                        {this.state.userList.map((item) =>\n                            <Card.Group key={item._id}>\n\n                                {item.seller == true\n\n                                    ? <Card >\n                                        <Card.Content>\n                                            <Image\n                                                width='280'\n                                                height='200'\n                                                src={item.avatar}\n                                            />\n                                            <Card.Header>{item.firstname} {