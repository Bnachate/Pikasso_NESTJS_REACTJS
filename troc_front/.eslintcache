    favorites !== undefined ?\n\n            favorites.map(element =>\n\n\n              <div className=\"artworkCard\" key={element._id}>\n\n                <div className=\"imageCard\">\n                  <img src={element.image} alt=\"artwork image\" />\n                </div>\n                <div className=\"textCard\">\n                  <h4>Type:</h4>\n                  <p>{element.type}</p>\n                </div>\n                <div className=\"textCard\">\n                  <h4>Price:</h4>\n                  <p>{element.price} euros</p>\n                </div>\n                <div className=\"textCard\">\n                  <h4>Description:</h4>\n                  <p>{element.description}</p>\n                </div>\n                <div className=\"actionsCard\">\n                  <button className=\"delete\" type=\"button\" onClick={(e) => deleteUserFavorite(e, element._id, idMain)}>Delete</button>\n\n                </div>\n\n              </div>\n            )\n            : null\n        }\n      </div>\n\n\n\n    </div>\n  );\n}\n\nexport default MyFavorites;\n","/home/brahim/C-COD-260-PAR-2-1-ecp-sarah.leleux/troc_front/src/components/dashboardSeller/myArtworks.js",["453","454","455"],"import axios from 'axios';\nimport React, { useState, useEffect } from 'react'\nimport { useHistory, Link } from \"react-router-dom\"\nimport '../../App.css'\n\n\n\nfunction MyArtworks({ idMain }) {\n\n  const [artworks, setArtworks] = useState()\n  const [noArtwork, setnoArtwork] = useState(true)\n  const [userID, setUserID] = useState(idMain)\n  let history = useHistory()\n  // const userID=idMain\n  const url = \"https://pikassoback.herokuapp.com/users/artworks/\" + userID\n\n\n  useEffect(() => {\n    getUserArtworks()\n  }, [])\n\n\n  function getUserArtworks() {\n\n    axios({\n      method: 'GET',\n      url: url,\n    })\n      .then(response => {\n        if (response.data.length > 0) {\n          setArtworks(response.data)\n          setnoArtwork(false)\n